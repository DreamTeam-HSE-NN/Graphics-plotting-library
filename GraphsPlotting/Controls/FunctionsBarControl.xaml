<UserControl x:Class="GraphsPlotting.Controls.FunctionsBarControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraphsPlotting.Controls" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" d:DataContext="{d:DesignInstance Type=local:FunctionsBarModel}"
             mc:Ignorable="d" d:DesignHeight="61.293" d:DesignWidth="138.333" >
    <Border Style="{StaticResource BorderDefaultStyle}">
        <WrapPanel>
            <xctk:DropDownButton Content="Trigonometry">
                <xctk:DropDownButton.DropDownContent>
                    <ItemsControl ItemsSource="{Binding Path=Commands}"/>
                </xctk:DropDownButton.DropDownContent>
            </xctk:DropDownButton>
        </WrapPanel>
    </Border>
</UserControl>
